{
  "expo": {
    "name": "Pigmy_Pro",
    "slug": "Pigmy_Pro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/favic.png",
    "scheme": "pigmypro",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/favic.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favic.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
